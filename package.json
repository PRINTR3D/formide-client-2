{
  "name": "formide-client-2",
  "version": "2.1.1-alpha.2",
  "description": "Client software that connects 3D printers to Formide Cloud Platform",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "author": "Chris ter Beke <chris@printr.com>",
  "license": "GPL-2.0",
  "scripts": {
    "docs": "./node_modules/.bin/apidoc -i ./src/interfaces/http -e node_modules -i ~/formide/plugins -e node_modules -o ./docs/",
    "dev": "nodemon --ignore ./src/app --ignore ./src/www/public --ignore ./tmp --ignore Gruntfile.js app",
    "debug": "NODE_ENV=development DEBUG=app:*,plugin:* node app",
    "test2": "NODE_ENV=test mocha --timeout=10000 test/index.test.js",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec && open coverage/lcov-report/index.html",
    "dev:mac": "OS_IMPLEMENTATION=mac_os npm run dev",
    "debug:mac": "OS_IMPLEMENTATION=mac_os npm run debug",
    "test:mac": "OS_IMPLEMENTATION=mac_os npm run test2",
    "coverage:mac": "OS_IMPLEMENTATION=mac_os npm run coverage",
    "start": "NODE_ENV=production node app",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
